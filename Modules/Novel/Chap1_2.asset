  - _characterName: "C\u7D75\u91CC"
    _text: "\u3061\u3087\u3046\u3069\uFF14\u4EBA\u3044\u308B\u3057\u3001\n\u3053\u306E\u30E1\u30C0\u30EB\u30B2\u30FC\u30E0\u3084\u3089\u306A\u3044\u30FC\uFF1F"
    _characterSprite: {fileID: 21300000, guid: f3dbf69d9e2e2ee41aad2ae7b913d163, type: 3}
  - _characterName: "\u5343A\u91CC"
